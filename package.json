{
  "name": "nockback-harder",
  "version": "4.0.0",
  "license": "MIT",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --config=jest.config.json --runInBand",
    "test:update-snapshots": "npm run test -- -u",
    "test:coverage": "jest --config=jest.config.json --coverage --runInBand",
    "test:ci": "npm run eslint && npm run test:coverage",
    "eslint": "eslint --format codeframe \"lib/**/*.ts\" \"test/**/*.ts\"",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\"",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "nock": "^11.3.3",
    "validation-utils": "^4.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^10.14.17",
    "@types/rimraf": "^2.0.2",
    "@types/superagent": "^4.1.3",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "express": "^4.17.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "superagent": "^5.1.0",
    "ts-jest": "^24.0.2",
    "typescript": "3.5.3"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "statements": 85,
        "branches": 75,
        "functions": 90,
        "lines": 85
      }
    }
  },
  "homepage": "http://github.com/kibertoad/nockback-harder",
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/nockback-harder.git"
  },
  "keywords": [
    "nock",
    "mock",
    "mocking",
    "http",
    "testing",
    "nockback"
  ],
  "files": [
    "README.md",
    "LICENSE",
    "dist/*"
  ]
}
