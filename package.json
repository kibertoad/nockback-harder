{
  "name": "nockback-harder",
  "version": "5.0.1",
  "license": "MIT",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --config=jest.config.json --runInBand",
    "test:update-snapshots": "npm run test -- -u",
    "test:coverage": "jest --config=jest.config.json --coverage --runInBand",
    "test:ci": "npm run lint && npm run test:coverage",
    "lint": "eslint --format codeframe \"lib/**/*.ts\" \"test/**/*.ts\"",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\" index.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "nock": "^13.0.11",
    "validation-utils": "^6.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.0",
    "@types/superagent": "^4.1.11",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "superagent": "^6.1.0",
    "ts-jest": "^26.5.6",
    "typescript": "4.2.4"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "statements": 94,
        "branches": 87,
        "functions": 92,
        "lines": 94
      }
    }
  },
  "homepage": "http://github.com/kibertoad/nockback-harder",
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/nockback-harder.git"
  },
  "keywords": [
    "nock",
    "mock",
    "mocking",
    "http",
    "testing",
    "nockback",
    "nock"
  ],
  "files": [
    "README.md",
    "LICENSE",
    "dist/*"
  ]
}
