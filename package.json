{
  "name": "nockback-harder",
  "version": "2.0.0",
  "license": "MIT",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "del-cli dist/ && tsc",
    "test": "jest --config=jest.config.json --runInBand",
    "test:coverage": "jest --config=jest.config.json --coverage --runInBand",
    "test:ci": "npm run eslint && npm run test:coverage",
    "eslint": "eslint --format codeframe \"lib/**/*.ts\" \"test/**/*.ts\"",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\""
  },
  "dependencies": {
    "nock": "^10.0.6",
    "validation-utils": "^2.8.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.13",
    "@types/nock": "^10.0.3",
    "@types/node": "^10.14.6",
    "@types/rimraf": "^2.0.2",
    "@types/superagent": "^4.1.1",
    "del-cli": "^1.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "express": "^4.16.4",
    "jest": "^24.8.0",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "superagent": "^5.0.5",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "statements": 85,
        "branches": 75,
        "functions": 90,
        "lines": 85
      }
    }
  },
  "homepage": "http://github.com/kibertoad/nockback-harder",
  "keywords": [
    "nock",
    "mock",
    "mocking",
    "http",
    "testing",
    "nockback"
  ]
}
